{
  "name": "telegram_bot_api",
  "version": "0.1.0",
  "description": "Node.js Telegram Bot API (ESNext)",
  "main": "index.js",
  "typescript": {
    "main": "index.ts"
  },
  "typings": "index.d.ts",
  "scripts": {
    "postinstall": "typings install",
    "start": "node .",
    "build": "tsc -p .",
    "test": "mocha --compilers js:babel-register --reporter 'spec' src/**/*[sS]pec.js examples/*[sS]pec.js",
    "watchtest": "mocha --compilers js:babel-register --reporter 'spec' --watch src/**/*[sS]pec.js examples/*[sS]pec.js",
    "watch": "tsc -w -p .",
    "preversion": "npm run build && npm test",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliMD/Node.js-Telegram-Bot-API.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "telegram bot",
    "api",
    "alimd",
    "ali.md",
    "ali",
    "mihandoost",
    "1devs"
  ],
  "author": "Ali Mihandoost <i@ali.md> (http://ali.md/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AliMD/Node.js-Telegram-Bot-API/issues"
  },
  "homepage": "https://github.com/AliMD/Node.js-Telegram-Bot-API#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^4.11.2",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "expect.js": "^0.3.1",
    "growl": "^1.9.2",
    "jasmine": "^2.4.1",
    "mocha": "^2.4.5",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
